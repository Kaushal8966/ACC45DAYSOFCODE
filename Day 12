// Write a program that reads a 5*5 arrray of intigers and then print the row sum and the column sum
#include <stdio.h>

int main() {
    int arr[5][5] = {
        {8,3,9,0,10},
        {3,5,17,1,1},
        {2,8,6,23,1},
        {15,7,3,2,9},
        {6,14,2,6,0}
        
    };
    int i,j,sum =0;
    printf("Row total");
    for(i=0;i<5;i++){
        for(j=0;j<5;j++){
            sum += arr[i][j];
        }
            printf("%d\n",sum);
            sum=0;
    }
    printf("Column total");
    for(i=0;i<5;i++){
        for(j=0;j<5;j++){
            sum  += arr[i][j];
        }
            printf("%d\n",sum);
        sum=0;
}
    return 0;
}
