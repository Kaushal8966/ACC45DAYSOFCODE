// Q1- sum of first and last digit
#include <stdio.h>

int main() {
    int i,n,firstdigit,lastdigit,sum = 0;
    printf("enter the number of firstdigit and lastdigit");
    scanf("%d",&n);
    lastdigit=n%10;
    while(n>=10){
        
        n=n/10;
    }
    firstdigit=n;
        printf("firstdigit = %d lastdigit =%d",firstdigit,lastdigit);
        sum =firstdigit + lastdigit;
    printf("sum of first and last digit is %d\n",sum);
    

    return 0;
}
