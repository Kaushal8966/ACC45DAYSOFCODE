// palindrome number
#include <stdio.h>

int main() {
    int i,n,r,s=0;
    // Write C code here
    printf("Enter the number");
    scanf("%d",&n);
    for(i=n;i>0;i--){
        r=i%10;
        s=s*10+r;
        i=i/10;
    }
    if(s==n){
        printf("%d is palindrome number",n);
    }
    else{
    printf("%d is not palindrome number",n);
    }
    return 0;
}
