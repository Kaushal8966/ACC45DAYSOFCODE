// Q-1 fibnocii series using loop
#include<stdio.h>

 
int main ()
{
 
  int a = 0 ,b =1 , c, i, n;
 
  printf ("Enter the length of the fibonacci series \n");
 
  scanf ("%d", &n);
 
  printf ("The Fibonacci series is :\n");
 
  printf ("%d%d", a, b);
 
  for (i = 2; i <= n; i++)
 
    {
 
      c = a + b;
 
      printf ("%d ", c);
 
     a = b;
 
      b = c;
 
    }
 
  return 0;
 
}
